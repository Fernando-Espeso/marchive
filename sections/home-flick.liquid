
{%- for block in section.blocks -%}
    {% case block.type %}
        {% when 'single' %}
           <div class="home_img">
                  <img alt="{{ block.settings.single.alt}}" src="{{ block.settings.single | img_url: 'master' }}" />
           </div>
    {% endcase %}
{% endfor %}

{% schema %}
{
  "name": "banner",
  "tag": "div",
  "class": "banner",
  "blocks": [
    {
      "type": "double",
      "name": "double",
      "limit": 2,
      "settings": [
        {
          "type": "image_picker",
          "id": "double",
          "label": "item"
        }
      ]
    }
  ]
}
{% endschema %}
