{%- for block in section.blocks -%}
    {% case block.type %}
        {% when 'image' %}
          <img alt="{{ block.settings.image.alt}}" src="{{ block.settings.image | img_url: 'master' }}" />
        {% endcase %}
    {% endfor %}
{% schema %}

{
  "name": "flick",
  "tag": "div",
  "class": "flick",
  "blocks": [
    {
      "type": "image",
      "name": "image",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "item"
        }
      ]
    }
  ]
}
{% endschema %}
